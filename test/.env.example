# =========================================
# 环境变量配置示例
# 复制此文件为 .env 并修改配置
# =========================================

# MySQL 数据库配置
MYSQL_ROOT_PASSWORD=your_strong_root_password_here
MYSQL_DATABASE=botnet_db
MYSQL_USER=botnet
MYSQL_PASSWORD=your_strong_password_here

# 后端配置
API_KEY=your_api_key_here
ALLOWED_UPLOAD_IPS=  # 留空表示允许所有IP，或设置如: 1.2.3.4,5.6.7.8

# 时区
TZ=Asia/Shanghai
